{
  "name": "tilelive-s3",
  "version": "1.4.1-nan",
  "dependencies": {
    "aws-sdk": "~2.0.17",
    "knox": "0.8.x",
    "locking": "^2.0.0",
    "nan": "^1.8.4",
    "node-pre-gyp": "~0.6.7",
    "tilejson": "0.13.x",
    "tiletype": "0.1.x"
  },
  "bundledDependencies": [
    "node-pre-gyp"
  ],
  "devDependencies": {
    "hat": "0.0.3",
    "tape": "3.0.x"
  },
  "main": "./lib/index.js",
  "scripts": {
    "install": "node-pre-gyp install --fallback-to-build",
    "test": "tape test/*.test.js"
  },
  "binary": {
    "module_name": "tilelive_s3",
    "module_path": "./lib/binding/",
    "host": "https://mapbox-node-binary.s3.amazonaws.com",
    "remote_path": "./{name}/v{version}",
    "package_name": "{node_abi}-{platform}-{arch}.tar.gz"
  }
}
